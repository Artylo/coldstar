            <string>///Controlls

if(keyboard_check_pressed(vk_escape)) game_end();
if(keyboard_check_pressed(ord('R'))) game_restart();

/*Scroll*/
var scroll = 24;
var ease = scroll;
if(mouse_wheel_up())
{
    portmod -= scroll/room_speed;
    view_xview += ease;
    view_yview += ease;
}
if(mouse_wheel_down()) 
{
    portmod += scroll/room_speed;
    view_xview -= ease;
    view_yview -= ease;
}

if(mouse_check_button(mb_middle)) and (portmod != global.genmod)
{
 if(portmod &lt; global.genmod) portmod += scroll/room_speed/2;
 else portmod -= scroll/room_speed/2;
}

/*Side Camera*/
var border = 32;
var mx = window_mouse_get_x();
var my = window_mouse_get_y();
if(mx &lt;= border) view_xview -= scroll;
if(mx &gt;= window_get_width()-border) view_xview += scroll;
if(my &lt;= border) view_yview -= scroll;
if(my &gt;= window_get_height()-border) view_yview += scroll;

if(keyboard_check_pressed(ord('C')))
{
    view_xview = nearsun.x - view_wview/2;
    view_yview = nearsun.y - view_hview/2;
}


</string>
